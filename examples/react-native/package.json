{
  "name": "ElectricSQLExample",
  "version": "0.0.1",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "scripts": {
    "backend:pull": "docker pull electricsql/electric:latest",
    "backend:down": "docker compose -f backend/compose/docker-compose.yaml down --volumes",
    "backend:start": "yarn backend:pull && docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml up",
    "backend:stop": "yarn backend:down",
    "backend:up": "yarn backend:start --detach",
    "client:generate": "npx electric-sql generate",
    "client:watch": "npx electric-sql generate --watch",
    "db:migrate": "node ./db/migrate.js",
    "db:psql": "node ./db/connect.js",
    "electric:start": "node ./backend/startElectric.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pods:install": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "test": "jest"
  },
  "dependencies": {
    "electric-sql": "latest",
    "react": "^18.2.0",
    "react-native": "0.72.4",
    "react-native-base64": "^0.2.1",
    "react-native-get-random-values": "^1.9.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-sqlite-storage": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@databases/pg": "^5.4.1",
    "@electric-sql/prisma-generator": "latest",
    "@prisma/client": "4.8.1",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.24",
    "@types/react-native-sqlite-storage": "^6.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "prisma": "4.8.1",
    "react-test-renderer": "18.2.0",
    "shelljs": "^0.8.5",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16.11.0"
  }
}
