{
  "name": "electric-sql-expo-example",
  "version": "0.9.4",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "scripts": {
    "backend:start": "export $(cat .env | xargs) && docker compose -f compose.yaml up",
    "backend:stop": "npx electric-sql stop",
    "backend:up": "npx electric-sql start --detach",
    "backend:down": "npx electric-sql stop --remove",
    "client:generate": "npx electric-sql generate",
    "client:watch": "npx electric-sql generate --watch",
    "db:migrate": "npx electric-sql with-config \"npx pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./db/migrations\"",
    "db:psql": "npx electric-sql psql",
    "electric:start": "npx electric-sql start",
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "lint": "eslint ."
  },
  "dependencies": {
    "@expo/html-elements": "^0.9.1",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/drawer": "^6.6.6",
    "@supabase/supabase-js": "^2.39.3",
    "@types/base-64": "^1.0.2",
    "base-64": "^1.0.0",
    "deep-equal": "^2.2.3",
    "electric-sql": "^0.9.4",
    "expo": "~50.0.6",
    "expo-camera": "~14.0.4",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-image-picker": "~14.7.1",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.7",
    "expo-sqlite": "~13.2.2",
    "expo-status-bar": "~1.11.1",
    "react": "^18.2.0",
    "react-native": "0.73.4",
    "react-native-base64": "^0.2.1",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-paper": "^5.12.3",
    "react-native-paper-dropdown": "^1.0.7",
    "react-native-qrcode-svg": "^6.3.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@databases/pg-migrations": "^5.0.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/deep-equal": "^1.0.4",
    "@types/react": "^18.0.18",
    "@types/react-native-base64": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-universe": "^12.0.0",
    "prettier": "^3.2.5",
    "react-native-svg-transformer": "^1.3.0",
    "typescript": "^5.1.3"
  }
}
