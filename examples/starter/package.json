{
  "name": "create-electric-app",
  "version": "0.2.8",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "type": "module",
  "platform": "node",
  "files": [
    "dist",
    "templates/*/**"
  ],
  "bin": {
    "create-electric-app": "dist/index.js"
  },
  "scripts": {
    "build": "pnpm run make-templates && shx rm -rf ./dist && tsc && tsmodule build",
    "prepublishOnly": "pnpm run build",
    "make-templates": "node ./scripts/make-templates.js",
    "format": "prettier --write --loglevel warn .",
    "e2e": "ava e2e/**/*.test.js"
  },
  "devDependencies": {
    "@tsmodule/tsmodule": "^44.7.0",
    "@types/shelljs": "^0.8.15",
    "@types/tcp-port-used": "^1.0.4",
    "ava": "^6.1.2",
    "prettier": "3.2.5",
    "shx": "^0.3.4",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "ora": "^8.0.1",
    "prompt": "^1.3.0",
    "shelljs": "^0.8.5",
    "tcp-port-used": "^1.0.2"
  }
}
