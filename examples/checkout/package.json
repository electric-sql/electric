{
  "name": "checkout",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "backend:start": "cd ./backend && docker compose up",
    "backend:stop": "cd ./backend && docker compose stop",
    "backend:restart": "cd ./backend && docker compose restart",
    "backend:up": "cd ./backend && docker compose up -d",
    "backend:down": "cd ./backend && docker compose -f docker-compose.yml -f ./dev/docker-compose.dev.yml down -v --remove-orphans",
    "client:generate": "electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/postgres",
    "client:watch": "npm run client:generate --watch",
    "db:migrate": "electric-sql with-config \"npx pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./db/migrations\"",
    "db:load-data": "node ./db/load-data.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "format": "prettier --write --loglevel warn .",
    "lint": "eslint"
  },
  "dependencies": {
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@databases/pg": "^5.5.0",
    "@databases/pg-migrations": "^5.0.2",
    "@ionic/react": "^7.8.5",
    "@ionic/react-router": "^7.8.5",
    "@maskito/core": "^1.9.0",
    "@maskito/react": "^1.9.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.42.5",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "dotenv-flow": "^4.0.1",
    "electric-sql": "^0.10.0",
    "ionicons": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "uuid": "^9.0.1",
    "wa-sqlite": "rhashimoto/wa-sqlite"
  },
  "devDependencies": {
    "@capacitor/cli": "5.5.1",
    "@databases/pg-migrations": "^5.0.3",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-legacy": "^5.3.2",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^12.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "jsdom": "^22.1.0",
    "supabase": "^1.163.2",
    "terser": "^5.30.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^0.32.2"
  }
}
