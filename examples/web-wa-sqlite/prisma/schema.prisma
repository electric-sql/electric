generator client {
  provider = "prisma-client-js"
}

generator electric {
  provider      = "./node_modules/prisma-generator-electric/dist/bin.js"
  output        = "../src/generated/models"
  relationModel = "false"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DB_URL")
}

model items {
  value String @id
}
