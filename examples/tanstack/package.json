{
  "name": "@electric-examples/tanstack",
  "private": true,
  "version": "0.0.1",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "backend:up": "PROJECT_NAME=tanstack-example pnpm -C ../../ run example-backend:up && pnpm db:migrate",
    "backend:down": "PROJECT_NAME=tanstack-example pnpm -C ../../ run example-backend:down",
    "build": "vite build",
    "db:migrate": "dotenv -e ../../.env.dev -- pnpm exec pg-migrations apply --directory ../.shared/db/migrations",
    "dev": "concurrently \"vite\" \"node src/server/app.js\"",
    "start": "node src/server/app.js",
    "format": "eslint . --fix",
    "preview": "vite preview",
    "stylecheck": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test:browser": "playwright install && playwright test ./e2e/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@electric-sql/client": "workspace:*",
    "@electric-sql/react": "workspace:*",
    "@tanstack/query-sync-storage-persister": "^5.52.2",
    "@tanstack/react-query": "^5.52.2",
    "@tanstack/react-query-persist-client": "^5.52.2",
    "pg": "^8.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.3",
    "@playwright/test": "^1.52.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "playwright": "^1.52.0",
    "sst": "3.6.35",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  }
}
