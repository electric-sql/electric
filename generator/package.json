{
  "name": "prisma-generator-electric",
  "version": "1.0.0",
  "description": "A Prisma generator that creates a DB description object using the schemas generated by zod-prisma-types.",
  "main": "dist/generator.js",
  "types": "dist/generator.d.ts",
  "keywords": [
    "zod",
    "prisma",
    "typescript",
    "schema",
    "types",
    "generator",
    "validation",
    "electric"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chrishoermann/zod-prisma-types.git"
  },
  "bin": {
    "zod-prisma-types": "dist/bin.js"
  },
  "scripts": {
    "lint": "eslint --ext ts,tsx,js,jsx --fix .",
    "test": "vitest",
    "build": "tsc",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "check-styleguide": "eslint --ext ts,tsx,js,jsx --quiet ."
  },
  "devDependencies": {
    "@prisma/internals": "^4.11.0",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-universe": "^11.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vitest": "^0.24.5"
  },
  "dependencies": {
    "@prisma/generator-helper": "^4.11.0",
    "code-block-writer": "^11.0.3",
    "lodash": "^4.17.21",
    "zod": "^3.21.1"
  }
}
