{
  "name": "@core/sync-service",
  "private": true,
  "version": "1.0.0-beta.4",
  "scripts": {
    "mix:write-static-version": "sed -i \"/version.exs/c\\ \\ version = $(elixir -r version.exs -e 'IO.inspect Electric.Version.version()')\" mix.exs",
